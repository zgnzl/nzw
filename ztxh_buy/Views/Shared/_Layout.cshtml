                    <li>@Html.ActionLink("产品", "GetPage", new { ID = "产品" })</li>
                    @*<li>@Html.ActionLink("案例", "GetPage", new { ID = "案例" })</li>*@
                    <li>@Html.ActionLink("技术", "GetPage", new { ID = "技术" })</li>
                    <li>@Html.ActionLink("资料下载", "GetPage", new { ID = "资料下载" })</li>
                    <li>@Html.ActionLink("联系我们", "GetPage", new { ID = "联系我们" })</li>
                </ul>
            </div>
        </div>
    </div>
    <marquee behavior="alternate" scrollamount="8"  height="30" bgcolor="#0099FF" style="margin-top: 30px;color: red;background: none;font-size: x-large;width:100%;">
    &hearts;&nbsp;双十&bull;一促销火爆进行中&nbsp;&hearts;</marquee>
    <div class="container body-content" style="margin-top:5px;">
        @RenderBody()
        <hr />
