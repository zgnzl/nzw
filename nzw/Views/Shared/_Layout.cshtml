<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="baidu-site-verification" content="bYrkJLk9pA" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="@ViewBag.KeyWords" />
    <meta name="description" content="@ViewBag.Description" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
@{
    ViewBag.KeyWords = "北京中泰祥和科技有限公司,云桌面,虚拟化,机房管理,电子元器件,硬盘保护";
    ViewBag.Description = "北京中泰祥和科技有限公司，专门为机房提供云桌面，虚拟化，远程管理，高校机房管理，电子元器件，硬盘保护；经理：聂立，联系电话：15801473663";
    ViewBag.CompanyName = "北京-中泰祥和科技有限公司";
}
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
<h1 id="CompanyName" style="color:white;font-weight:600">@ViewBag.CompanyName</h1>
            </div>
            <div class="navbar-collapse collapse" style="float:right">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("主页", "Index", "Home")</li>
                    <li>@Html.ActionLink("产品", "Product", "Home")</li>
                    <li>@Html.ActionLink("关于", "About", "Home")</li>
                    <li>@Html.ActionLink("联系", "Contact", "Home")</li>
                    <li>@Html.ActionLink("招聘", "ZhaoPin", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>
                &copy; @DateTime.Now.Year - @ViewBag.CompanyName    版权所有
                <script type="text/javascript">
                    document.write(unescape(" %3Cscript type='text/javascript' src='http://zg69.cn/statictics/index/testid?title=" + escape(document.title) + "&sourceurl=" + encodeURI(document.location.href) + "'%3E%3C/script%3E"));
                </script>
            </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
