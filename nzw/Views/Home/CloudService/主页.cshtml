<script>
    $(function () {
        $("footer").parent("div").width(1811);
        function paly()
        {
            var index = $("#et-slider-controls .et-active-control").index();
            $("#et-slides div").eq(index).hide();
            $("#et-slider-controls").find("a").eq(index).removeClass("et-active-control");
            if (index == 3) {
                $("#et-slides div").eq(0).show();
                $("#et-slider-controls").find("a").eq(0).addClass("et-active-control");
            } else {
                $("#et-slides div").eq(index + 1).show();
                $("#et-slider-controls").find("a").eq(index + 1).addClass("et-active-control");
            }
        }
        var interval =setInterval(function () {
            paly();  
        }, 5000);
        $("#et-slider-controls a").click(function () {
            clearInterval();
            setTimeout(function () {
                interval = setInterval(function () {
                    paly();
                }, 5000);},9000);
            var index1 = $("#et-slider-controls .et-active-control").index();
            var index = $(this).index();
            $("#et-slides div").eq(index1).hide();
            $("#et-slider-controls").find("a").eq(index1).removeClass("et-active-control");
            $("#et-slides div").eq(index).show();
            $("#et-slider-controls").find("a").eq(index).addClass("et-active-control");
          
        });
    });
</script>
